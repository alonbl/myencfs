#include <winresrc.h>

VS_VERSION_INFO VERSIONINFO
	FILEVERSION @MYENCFS_VERSION_MAJOR@,@MYENCFS_VERSION_MINOR@,@MYENCFS_VERSION_FIX@,@MYENCFS_VERSION_REVISION@
	PRODUCTVERSION @MYENCFS_VERSION_MAJOR@,@MYENCFS_VERSION_MINOR@,@MYENCFS_VERSION_FIX@,@MYENCFS_VERSION_REVISION@
	FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
	FILEFLAGS 0x21L
#else
	FILEFLAGS 0x20L
#endif
	FILEOS 0x40004L
	FILETYPE 0x2L
	FILESUBTYPE 0x0L
BEGIN
	BLOCK "StringFileInfo"
	BEGIN
		BLOCK "040904b0"
		BEGIN
			VALUE "FileVersion", "@MYENCFS_VERSION_MAJOR@.@MYENCFS_VERSION_MINOR@.@MYENCFS_VERSION_FIX@.@MYENCFS_VERSION_REVISION@"
			VALUE "InternalName", "@PACKAGE_NAME@"
			VALUE "ProductName", "@PACKAGE_NAME@"
			VALUE "ProductVersion", "@MYENCFS_VERSION_MAJOR@,@MYENCFS_VERSION_MINOR@,@MYENCFS_VERSION_FIX@,@MYENCFS_VERSION_REVISION@"
			VALUE "FileDescription", "@PACKAGE_NAME@-@PACKAGE_VERSION@ (@PACKAGE_BUILD_ID@) Library"
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x409, 1200
	END
END
